{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "JoineryServer": "Debug"
    }
  },
  "Authentication": {
    "GitHub": {
      "ClientId": "your-github-client-id-for-dev",
      "ClientSecret": "your-github-client-secret-for-dev"
    },
    "Microsoft": {
      "Instance": "https://login.microsoftonline.com/",
      "TenantId": "your-dev-tenant-id",
      "ClientId": "your-dev-microsoft-client-id",
      "ClientSecret": "your-dev-microsoft-client-secret",
      "CallbackPath": "/signin-microsoft"
    }
  },
  "JWT": {
    "SecretKey": "development-secret-key-for-jwt-tokens-must-be-at-least-256-bits-long",
    "Issuer": "JoineryServer-Dev",
    "Audience": "JoineryClients-Dev",
    "ExpirationHours": 1
  }
}